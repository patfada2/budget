<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1024.0" styleClass="root" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="loadmenu" mnemonicParsing="false" onAction="#loadExports" text="load exports" />
                  <MenuItem fx:id="savemenu1" mnemonicParsing="false" onAction="#save" text="save" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Tools">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#showCategoryManager" text="Category Manager" />
                  <MenuItem mnemonicParsing="false" onAction="#runMatches" text="Run matches" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <AnchorPane prefHeight="68.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="labeutton1l1" layoutY="21.0" mnemonicParsing="false" onAction="#HelloButtonClicked" text="Button" />
            <Label layoutX="346.0" layoutY="13.0" text="Label" />
            <Label layoutX="346.0" layoutY="38.0" text="Label" />
            <Label layoutX="587.0" layoutY="38.0" text="Label" />
         </children>
      </AnchorPane>
   </bottom>
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab onSelectionChanged="#onSelectChartTab" text="Table">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="table1" layoutY="-8.0" prefHeight="800.0" prefWidth="1024.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="charttab" text="Chart">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <PieChart fx:id="pieChart" />
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
